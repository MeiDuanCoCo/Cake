@{
    Layout = null;
}
@model IList<Cake.Dto.Cake.ScenarioDto>

<nav class="navbar navbar-default" role="navigation">
<div class="navbar-header">
    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span> 
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
    </button>
    <h1 class="navbar-brand">
        <a href="/">Yummy</a>
    </h1>
</div>
<!--navbar-header-->
<div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
<ul class="nav navbar-nav">
<li>
    <a href="/" class="active">首页</a>
</li>
    
    @foreach (var scen in Model)
    {
        <li class="dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown">@scen.ScenarioName<b class="caret"></b></a>
            <ul class="dropdown-menu multi-column columns-4">
                <li>
                    <div class="row">
                        @foreach (var title in scen.CakeTitles)
                        {
                            <div class="col-sm-3">
                                <h4>@title.Name</h4>
                                <ul class="multi-column-dropdown">
                                    @foreach (var con in scen.CakeTypes)
                                    {
                                        <li>
                                            <a class="list" href="@con.Url">@con.Name</a>
                                        </li>
                                    }
                                </ul>
                            </div>
                        }
                    </div>
                </li>
            </ul>
        </li>
    }


